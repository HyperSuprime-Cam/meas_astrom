# -*- python -*-
Import("env")

if False:
    env.Program(["Footprint_1.cc"], LIBS=env.getlibs("meas_astrom"))
#
# Tests
#
import lsst.tests

#don't run tests for the moment
if True:
    tests = lsst.tests.Control(env)
    tests.run("*.cc")

    for target in tests.run("*.py"):
        env.Depends(target, "#/python/lsst/meas/astrom/net/_netLib%s" % (env['LDMODULESUFFIX']))
